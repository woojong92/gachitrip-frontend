{
    "presets": [
        [
            "next/babel"
        ]
    ],
    "plugins": [
        [
            "emotion"
        ],
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "@components": "./src/components"
                }
            }
        ]
    ]
}

